{
  "name": "@percy/webdriverio",
  "version": "1.0.0",
  "description": "percy.io integration for webdriver.io",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "files": [
    "dist"
  ],
  "scripts": {
    "prepublishOnly": "npm run build",
    "clean": "rm -rf dist/",
    "build": "npm run clean && tsc",
