    "test-win": "npm run pretest && percy exec -- wdio.cmd wdio.conf.js"
  },
  "dependencies": {
    "@percy/agent": "^0.2.2-beta.4"
  },
  "devDependencies": {
    "@wdio/cli": "^5.7.4",
    "@wdio/local-runner": "^5.7.4",
    "@wdio/mocha-framework": "^5.7.3",
    "@wdio/selenium-standalone-service": "^5.7.2",
    "@wdio/spec-reporter": "^5.7.2",
    "@wdio/sync": "^5.7.3",
    "chai": "^4.2.0",
    "http-server": "^0.11.1",
    "mocha": "^6.0.2",
    "typescript": "^3.3.3333"
  },
